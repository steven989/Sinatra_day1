
<div class="header">

    <h1>Welcome to <%= @@crm_app_name %>!</h1>
        
    <a href="/">
    <div class="home_button">
    <h1>Home</h1>
    </div>
    </a>

</div>
<div class="header-background">



</div>

<div class="body">



    <div id="contacts">
    
        <h1>Here are all your contacts:</h1> 
        <% @filtered_array.each { |contact| %>
            <div class="individual_contact">
                <% id = contact.id %>
                <% name = contact.first_name+" "+contact.last_name %>
                <% email =contact.email %>
                <% notes = contact.notes %>
                <h1><%= name %></h1> <br>
                <p><%= email %> </p> <br>
                <p><%= notes %> </p>
            </div> 
        <% } %>
             
        
    </div>

</div>

<div class="footer">

<h2>It is <%= @@current_time %> right now.</h2>
</div>

<div class="footer-background">
</div>